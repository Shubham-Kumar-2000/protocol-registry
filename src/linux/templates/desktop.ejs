[Desktop Entry]
Type=Application
Name=<%= appName %> 
Exec=<%if (terminal) { %><%- command %>; exec bash <% } else { %><%- command %> <% } %>
StartupNotify=false
Terminal=<%= terminal  ? 'true' : 'false'  %>
MimeType=x-scheme-handler/<%= protocol %>;
PRIdentifier=com.protocol.registry.<%= protocol %>