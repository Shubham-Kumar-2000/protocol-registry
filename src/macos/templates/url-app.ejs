on open location this_URL
    <%if (terminal > 0) { %>
        do shell script "defaults write com.protocol.registry.<%- protocol %> current_url '" & this_URL & "'"
        tell application "<%- application %>" to activate
    <% } else { %>
        set command to "<%- command %>"
        do shell script "export PATH=/usr/local/bin:$PATH ; source ~/.zshrc ; zsh -lc \"" & command & "\""
    <% } %>
end open location